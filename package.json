{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "webtem": "bin/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --module-reason --config webpack.dev.js",
