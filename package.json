    "prettier": "prettier --write --config .prettierrc.js \"{bin,src,test,cypress,build}/**/*.{js,ts}\""
  },
  "author": "hahaaha",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "commander": "^6.2.0",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "metalsmith": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
